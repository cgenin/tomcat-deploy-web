{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "phantomjs": true,
    "es6": true
  },
  "rules": {
    "import/no-webpack-loader-syntax": 0,
    "no-use-before-define": 0,
    "no-console": 0,
    "new-cap": 0,
    "comma-dangle": [
      2,
      "never"
    ],
    "strict": 0,
    "max-len": [
      1,
      120
    ],
    "react/prop-types": 0,
    "react/jsx-boolean-value": [
      1,
      "Always"
    ],
    "react/no-multi-comp": 0,
    "react/jsx-no-bind": 0,
    "object-curly-spacing": [0, "never"],
    "indent": [1, 2,{"SwitchCase" :2}],
    "func-names":0,
    "object-shorthand":0,
    "quote-props":0,
    "eol-last":0,
    "react/jsx-closing-bracket-location":0,
    "react/jsx-indent-props":0
  }
}
